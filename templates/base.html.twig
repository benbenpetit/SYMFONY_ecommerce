<!DOCTYPE html>
<html class="is-loaded">
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Sheesh-irts® | Free delivery crazy week
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
		<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
	</head>
	<body>
		<div class="loader"></div>
		<div class="content" data-load-container>
			<header class="primary-nav">
				<nav aria-label="primary navigation">
					<ul>
						<li><a href="{{ path('home') }}">Home</a></li>
						<li><a href="{{ path('register') }}">Register</a></li>
						<li><a href="{{ path('login') }}">Login</a></li>
						<li><a href="{{ path('logout') }}">Logout</a></li>
						<li><a href="{{ path('cart') }}">Cart</a></li>
					</ul>
				</nav>
			</header>
			<div class="page" data-scroll-container>
				<main>
					{% block body %}
					{% endblock %}
				</main>
				<footer></footer>
			</div>
		</div>
	</body>
</html>
